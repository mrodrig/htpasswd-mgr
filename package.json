{
    "author": "mrodrig",
    "name": "htpasswd-mgr",
    "description": "HTPasswd Manager for Node.js",
    "version": "1.1.6",
    "homepage": "https://mrodrig.github.io/htpasswd-mgr",
    "repository": {
        "type": "git",
        "url": "http://github.com/mrodrig/htpasswd-mgr.git"
    },
    "main": "lib/manager.js",
