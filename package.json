{
    "author": "mrodrig",
    "name": "htpasswd-mgr",
    "description": "HTPasswd Manager for Node.js",
    "version": "2.0.2",
    "homepage": "https://mrodrig.github.io/htpasswd-mgr",
    "repository": {
        "type": "git",
        "url": "http://github.com/mrodrig/htpasswd-mgr.git"
    },
    "main": "lib/manager.js",
    "types": "./lib/manager.d.ts",
    "scripts": {
        "build": "npm run lint && npm run test && npm run compile",
        "compile": "tsc -p tsconfig.build.json",
        "coverage": "nyc npm run test",
        "lint": "eslint --ext .js,.ts src test",
        "prepublishOnly": "npm run build",
        "test": "mocha -r ts-node/register test/index.ts"
    },
    "keywords": [
        "htpasswd",
        "apache",
        "password",
        "file",
        "manager"
    ],
    "dependencies": {
        "apache-crypt": "1.2.6",
        "apache-md5": "1.1.8",
        "bcrypt": "5.1.1"
    },
    "devDependencies": {
        "@types/bcrypt": "5.0.2",
        "@types/mocha": "10.0.7",
        "@types/node": "20.14.10",
        "@typescript-eslint/eslint-plugin": "7.16.0",
        "@typescript-eslint/parser": "7.16.0",
        "eslint": "8.57.0",
        "eslint-config-google": "0.14.0",
        "eslint-plugin-import": "2.29.1",
        "mocha": "10.6.0",
        "nyc": "17.0.0",
        "ts-node": "10.9.2",
        "typescript": "5.5.3",
        "should": "13.2.3"
    },
    "engines": {
        "node": ">= 16"
    },
    "license": "MIT"
}
